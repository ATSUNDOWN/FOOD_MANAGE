prefix-icon=\"el-icon-lock\"\r\n            show-password\r\n            placeholder=\"请输入密码\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item prop=\"confirm\">\r\n          <el-input\r\n            v-model=\"form.confirm\"\r\n            prefix-icon=\"el-icon-lock\"\r\n            show-password\r\n            placeholder=\"请确认你的密码\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button style=\"width: 100%\" type=\"primary\" @click=\"register\"\r\n            >注 册</el-button\r\n          >\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\n\r\nexport default {\r\n  name: \"Register\",\r\n  data() {\r\n    return {\r\n      form: {},\r\n      rules: {\r\n        username: [{ required: true, message: \"请输入账号\", trigger: \"blur\" }],\r\n        password: [\r\n          { required: true, message: \"请输入密码\", trigger: \"blur\" },\r\n          {\r\n            min: 6,\r\n            max: 10,\r\n            message: \"长度在 6 到 10 个字符\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        confirm: [\r\n          { required: true, message: \"请输入确认密码\", trigger: \"blur\" },\r\n          {\r\n            min: 6,\r\n            max: 10,\r\n            message: \"长度在 6 到 10 个字符\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    register() {\r\n       if (this.form.password !== this.form.confirm) {\r\n        this.$message({\r\n          type: \"error\",\r\n          message: \"两次密码输入不一致,检查你的密码\",\r\n        });\r\n        return;\r\n      }\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          request.post(\"/user/register\", this.form).then((res) => {\r\n            if (res.code === \"0\") {\r\n              this.$message({\r\n                type: \"success\",\r\n                message: \"注册成功\",\r\n              });\r\n              this.$router.push(\"/login\"); //注册成功后进行跳转，跳转到登录页面\r\n            } else {\r\n              this.$message({\r\n                type: \"error\",\r\n                message: res.msg,\r\n              });\r\n            }\r\n          })\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]}]}