n type=\"primary\" style=\"margin-left: 5px\">查询</el-button>\r\n        </div>\r\n      </div>\r\n      <!-- 下滑栏 -->\r\n      <div style=\"width: 100px\">\r\n        <el-dropdown>\r\n          <span class=\"el-dropdown-link\">\r\n            {{ nickName }}<i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n          </span>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item @click.native=\"$router.push('/person')\"\r\n              >个人信息</el-dropdown-item\r\n            >\r\n            <el-dropdown-item @click.native=\"$router.push('/person')\"\r\n              >购物车</el-dropdown-item\r\n            >\r\n            <el-dropdown-item @click.native=\"$router.push('/login')\"\r\n              >退出系统</el-dropdown-item\r\n            >\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n    </div>\r\n    <!-- 中部导航栏 -->\r\n    <el-menu class=\"el-menu-demo\" mode=\"horizontal\" background-color=\"#409eff\">\r\n      <!-- <el-menu-item\r\n        v-for=\"(item, index) in cuisines\"\r\n        :key=\"index\"\r\n        style=\"width: 100px; color: white\"\r\n        >{{ item.cuisine }}</el-menu-item\r\n      > -->\r\n      <el-menu-item\r\n        style=\"color: white; margin-left: 100px\"\r\n        @click=\"$router.push('/home')\"\r\n        >主页\r\n      </el-menu-item>\r\n      <el-menu-item style=\"color: white\">闽菜 </el-menu-item>\r\n      <el-menu-item style=\"color: white\">粤菜 </el-menu-item>\r\n      <el-menu-item style=\"color: white\">鲁菜 </el-menu-item>\r\n      <el-menu-item style=\"color: white\">浙菜 </el-menu-item>\r\n      <el-menu-item style=\"color: white\">苏菜 </el-menu-item>\r\n    </el-menu>\r\n    <div style=\"display: flex\">\r\n      <div\r\n        style=\"\r\n          width: 800px;\r\n          height: 800px;\r\n          margin: 50px 0 0 25%;\r\n          border-radius: 20px;\r\n        \"\r\n      >\r\n        <div\r\n          style=\"\r\n            width: 500px;\r\n            height: 500px;\r\n            margin-left: 150px;\r\n            margin-top: 100px;\r\n          \"\r\n        >\r\n          <div style=\"margin-left:50px;\">\r\n            <div>\r\n              <h3 style=\"float: left;padding-top:10px\">用户名:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%;margin-bottom:50px\">{{ form.username }}</div>\r\n            </div>\r\n            <div>\r\n              <h3 style=\"float: left;padding-top:10px\">昵称:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ nickName }}</div>\r\n            </div>\r\n            <div style=\"margin-top:50px\">\r\n              <h3 style=\"float: left;padding-top:10px\">年龄:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ age }}</div>\r\n            </div>\r\n            <div style=\"margin-top:50px\">\r\n              <h3 style=\"float: left;padding-top:10px\">密码:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ password }}</div>\r\n            </div>\r\n            <div style=\"margin-top:50px\">\r\n              <h3 style=\"float: left;padding-top:10px\">性别:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ sex }}</div>\r\n            </div>\r\n            <div style=\"margin-top:50px\">\r\n              <h3 style=\"float: left;padding-top:10px\">地址:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ address }}</div>\r\n            </div>\r\n          </div>\r\n          <el-button type=\"primary\" style=\"margin:50px 40%\" @click=\"updatePerson\"\r\n        >修改</el-button>\r\n        </div>\r\n      </div>\r\n      <el-dialog\r\n        title=\"提示\"\r\n        :visible.sync=\"dialogVisible\"\r\n        width=\"30%\"\r\n        :before-close=\"handleClose\"\r\n      >\r\n        <el-form :model=\"form\" label-width=\"120px\">\r\n          <el-form-item label=\"用户名\">\r\n            <el-input v-model=\"form.username\" style=\"width: 80%\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"昵称\">\r\n            <el-input v-model=\"form.nickName\" style=\"width: 80%\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"年龄\">\r\n            <el-input v-model=\"form.age\" style=\"width: 80%\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"密码\">\r\n            <el-input v-model=\"form.password\" style=\"width: 80%\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"性别\">\r\n            <el-input v-model=\"form.sex\" style=\"width: 80%\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"地址\">\r\n            <el-input v-model=\"form.address\" style=\"width: 80%\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"save()\">确 定</el-button>\r\n        </span>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\nexport default {\r\n  name: \"Person\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      form: {},\r\n      username:\"\",\r\n      nickName: \"\",\r\n      password: \"\",\r\n      sex: \"\",\r\n      age: \"\",\r\n      address: \"\",\r\n      search: \"\",\r\n      dialogVisible:false\r\n    };\r\n  },\r\n  created() {\r\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\r\n    this.nickName = this.session.nickName;\r\n    this.password = this.session.password;\r\n    this.sex = this.session.sex;\r\n    this.age = this.session.age;\r\n    this.address = this.session.address;\r\n    this.username = this.session.username\r\n  },\r\n  methods: {\r\n    updatePerson(){\r\n      this.dialogVisible = true;\r\n      this.form = {};\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm(\"确认关闭？\")\r\n        .then((_) => {\r\n          done();\r\n        })\r\n        .catch((_) => {});\r\n    },\r\n    save() {\r\n      //更新\r\n      if (this.form.id) {\r\n        request.put(\"/user\", this.form).then((res) => {\r\n          console.log(res);\r\n          if (res.code == \"0\") {\r\n            this.$message({\r\n              type: \"success\",\r\n              message: \"更新成功\",\r\n            });\r\n          } else {\r\n            this.$message({\r\n              type: \"error\",\r\n              message: res.msg,\r\n            });\r\n          }\r\n          this.load();\r\n          this.dialogVisible = false;\r\n        });\r\n      } else {\r\n        request.post(\"/user\", this.form).then((res) => {\r\n          console.log(res);\r\n          if (res.code == \"0\") {\r\n            this.$message({\r\n              type: \"success\",\r\n              message: \"新增成功\",\r\n            });\r\n          } else {\r\n            this.$message({\r\n              type: \"error\",\r\n              message: res.msg,\r\n            });\r\n          }\r\n           this.load();//刷新表格数据\r\n        this.dialogVisible = false; //关闭弹窗\r\n        });  \r\n      }\r\n    },\r\n    // update() {\r\n    //   request.put(\"/user\", this.form).then((res) => {\r\n    //     console.log(res);\r\n    //     if (res.code === \"0\") {\r\n    //       this.$message({\r\n    //         type: \"success\",\r\n    //         message: \"更新\",\r\n    //       });\r\n    //       sessionStorage.setItem(\"user\", JSON.stringify(this.form));\r\n    //       this.$$emit(\"user\");\r\n    //     } else {\r\n    //       this.$message({\r\n    //         type: \"error\",\r\n    //         message: res.msg,\r\n    //       });\r\n    //     }\r\n    //   });\r\n    // },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}