{"ast":null,"code":"import request from \"@/utils/request.js\";\nexport default {\n  name: \"Myorder\",\n  data() {\n    return {\n      dialogVisible: false,\n      tableData: [],\n      total: 0,\n      username: \"\",\n      currentPage: 1,\n      pageSize: 10,\n      oid: \"\",\n      orderDetail: []\n    };\n  },\n  created() {\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\n    this.username = this.session.username;\n    this.load();\n  },\n  methods: {\n    checkOrderDetails(id, username) {\n      this.dialogVisible = true;\n      request.get(\"/ordering/checkOrderDetail\", {\n        params: {\n          id: id,\n          username: username\n        }\n      }).then(res => {\n        console.log(res);\n        this.orderDetail = res.data;\n        console.log(this.orderDetail);\n      });\n    },\n    load() {\n      request.get(\"/ordering/findMyOrderPage\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          username: this.username\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n        // console.log(this.tableData);\n      });\n    },\n\n    handleClose(done) {\n      this.$confirm(\"确认关闭？\").then(_ => {\n        done();\n      }).catch(_ => {});\n    },\n    updateOrderStatus(id) {\n      request.get(\"/ordering/updateOrderStatus\", {\n        params: {\n          status: 3,\n          id: id\n        }\n      }).then(res => {\n        this.load();\n        // console.log(res);\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"mappings":"AA+EA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;IACA;EACA;EACAC;IACAC;MACA;MACAC,QACAC;