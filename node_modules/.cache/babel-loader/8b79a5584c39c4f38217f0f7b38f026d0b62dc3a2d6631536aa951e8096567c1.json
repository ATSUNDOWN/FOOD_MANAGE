zeChange\"\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"currentPage\"\r\n      :page-sizes=\"[5, 10, 20]\"\r\n      :page-size=\"pageSize\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"total\"\r\n    >\r\n    </el-pagination>\r\n    <el-dialog\r\n      title=\"提示\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"30%\"\r\n      :before-close=\"handleClose\"\r\n    >\r\n      <div\r\n        v-for=\"(item, index) in orderDetail\"\r\n        :key=\"index\"\r\n        style=\"height: 120px\"\r\n      >\r\n        <div>\r\n          <el-image\r\n            :src=\"item.foodPic\"\r\n            style=\"height: 100px; width: 100px; float: left; margin-right: 50px\"\r\n          ></el-image>\r\n        </div>\r\n        <div style=\"line-height: 120px\">\r\n          <h2>{{ item.foodName }}</h2>\r\n        </div>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogVisible = false\"\r\n          >确 定</el-button\r\n        >\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\n\r\nexport default {\r\n  name: \"Myorder\",\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      tableData: [],\r\n      total: 0,\r\n      username: \"\",\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      oid: \"\",\r\n      orderDetail: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\r\n    this.username = this.session.username;\r\n    this.load();\r\n  },\r\n  methods: {\r\n    checkOrderDetails(id, username) {\r\n      this.dialogVisible = true;\r\n      request\r\n        .get(\"/ordering/checkOrderDetail\", {\r\n          params: {\r\n            id: id,\r\n            username: username,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          console.log(res);\r\n          this.orderDetail = res.data;\r\n          console.log(this.orderDetail);\r\n        });\r\n    },\r\n    load() {\r\n      request\r\n        .get(\"/ordering/findMyOrderPage\", {\r\n          params: {\r\n            pageNum: this.currentPage,\r\n            pageSize: this.pageSize,\r\n            username: this.username,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          this.tableData = res.data.records;\r\n          this.total = res.data.total;\r\n          // console.log(this.tableData);\r\n        });\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm(\"确认关闭？\")\r\n        .then((_) => {\r\n          done();\r\n        })\r\n        .catch((_) => {});\r\n    },\r\n    updateOrderStatus(id) {\r\n      request\r\n        .get(\"/ordering/updateOrderStatus\", {\r\n          params: {\r\n            status: 3,\r\n            id: id,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          this.load();\r\n          // console.log(res);\r\n        });\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      //改变当前每页的个数触发\r\n      this.pageSize = pageSize;\r\n      this.load();\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      //改变当前页码触发\r\n      this.currentPage = pageNum;\r\n      this.load();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}