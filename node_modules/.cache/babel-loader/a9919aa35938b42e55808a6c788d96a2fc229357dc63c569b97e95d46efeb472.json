{"ast":null,"code":"import request from \"@/utils/request.js\";\nexport default {\n  name: \"Cart\",\n  data() {\n    return {\n      tableData: [],\n      uid: \"\",\n      multipleSelection: [],\n      address: \"\",\n      foodName: \"\",\n      foodPic: \"\",\n      nickName: \"\",\n      sessionFood: \"\",\n      cout: 0\n    };\n  },\n  created() {\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\n    this.uid = this.session.username;\n    this.address = this.session.address;\n    // this.fid = this.sessionFood[0].fid;\n\n    // console.log(this.uid+this.address);\n    // console.log(this.session.username);\n    this.showCartInfo();\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/user/selectBalance\", {\n        params: {\n          username: this.uid\n        }\n      }).then(res => {\n        this.balance = res.data[0].balance;\n\n        // console.log(res);\n      });\n    },\n\n    finshShopping() {\n      if (this.balance >= this.totalPrice) {\n        let oid = this.cout + \"o\" + this.uid;\n        this.cout = this.cout + 1;\n        // console.log(oid);\n        request.get(\"/cart/finishShopping\", {\n          params: {\n            oid: oid,\n            username: this.uid,\n            address: this.address\n          }\n        }).then(res => {\n          request.get(\"/cart/deleteCartInfoByUid\", {\n            params: {\n              uid: this.uid\n            }\n          }).then(res => {\n            this.showCartInfo();\n            // console.log(res);\n          });\n          // console.log(res);\n          this.$message({\n            message: \"下单成功请等待配送\",\n            type: \"success\"\n          });\n        });\n      } else {\n        this.$message({\n          type: \"error\",\n          message: \"余额不足\"\n        });\n      }\n    },\n    showCartInfo() {\n      request.get(\"/cart/selectCartInfo\", {\n        params: {\n          uid: this.uid\n        }\n      }).then(res => {\n        // sessionStorage.setItem(\"food\", JSON.stringify(res.data));\n        this.tableData = res.data;\n        // console.log(this.foodInfo);\n        // console.log(this.tableData);\n        console.log(res);\n      });\n    },\n    increment(index) {\n      //加\n      // console.log(this.tableData);\n      // console.log(index);\n      this.tableData[index].fcount++;\n      console.log(this.tableData[index].fcount);\n      request.get(\"/cart/updateFoodNum\", {\n        params: {\n          fid: this.tableData[index].fid,\n          fcount: this.tabledata[index].fcount\n        }\n      }).then(res => {\n        console.log(res);\n      });\n    },\n    decrement(index) {\n      //减\n      this.tableData[index].fcount--;\n    },\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n    handleDelete(fid) {\n      request.get(\"/cart/deleteCartInfo\", {\n        params: {\n          fid: fid\n        }\n      }).then(res => {\n        if (res.code == \"0\") {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.showCartInfo();\n      });\n    }\n  },\n  computed: {\n    totalPrice() {\n      //计算总价\n      let total = 0;\n      for (let i in this.tableData) {\n        total += this.tableData[i].price * this.tableData[i].fcount;\n      }\n      return total;\n    }\n  },\n  filters: {\n    //过滤器\n    // 拼接价格/总价\n    showPrice(price) {\n      return \"￥\" + price.toFixed(2);\n    }\n  }\n};","map":{"version":3,"mappings":"AAmFA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;;IAEA;IACA;IACA;IACA;EACA;EACAC;IACAC;MACAC;QACAC;UACAC;QACA;MACA;QACA;;QAEA;MAEA;IACA;;IACAC;MACA;QACA;QACA;QACA;QACAH,QACAI;UACAH;YACAI;YACAH;YACAX;UACA;QACA,GACAe;UACAN;YACAC;cACAZ;YACA;UACA;YACA;YACA;UACA;UACA;UACA;YACAkB;YACAC;UACA;QAEA;MACA;QACA;UACAA;UACAD;QACA;MACA;IAEA;IACAE;MACAT,QACAI;QACAH;UACAZ;QACA;MACA,GACAiB;QACA;QACA;QACA;QACA;QACAI;MACA;IACA;IACAC;MACA;MACA;MACA;MACA;MACAD;MACAV;QACAC;UACAW;UACAC;QACA;MACA;QACAH;MACA;IACA;IACAI;MACA;MACA;IACA;IACAC;MACA;IACA;IACAC;MACAhB;QACAC;UACAW;QACA;MACA;QACA;UACA;YACAJ;YACAD;UACA;QACA;UACA;YACAC;YACAD;UACA;QACA;QACA;MACA;IACA;EACA;EACAU;IACAC;MACA;MACA;MACA;QACAC;MACA;MACA;IACA;EACA;EACAC;IACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","tableData","uid","multipleSelection","address","foodName","foodPic","nickName","sessionFood","cout","created","methods","load","request","params","username","finshShopping","get","oid","then","message","type","showCartInfo","console","increment","fid","fcount","decrement","handleSelectionChange","handleDelete","computed","totalPrice","total","filters","showPrice"],"sourceRoot":"src/views","sources":["Cart.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 购物车主界面 -->\r\n    <div>\r\n      <div>\r\n        <h2\r\n          style=\"\r\n            margin: 15px 20px;\r\n            color: #409eff;\r\n            border-bottom: 5px solid gray;\r\n            padding-bottom: 8px;\r\n          \"\r\n        >\r\n          购物车\r\n        </h2>\r\n      </div>\r\n      <div>\r\n        <el-table\r\n          ref=\"multipleTable\"\r\n          :data=\"tableData\"\r\n          tooltip-effect=\"dark\"\r\n          style=\"width: 100%\"\r\n          @selection-change=\"handleSelectionChange\"\r\n        >\r\n          <el-table-column prop=\"foodPic\" label=\"食品图片\">\r\n            <template slot-scope=\"scope\">\r\n              <div class=\"demo-image__preview\">\r\n                <el-image\r\n                  style=\"width: 100px; height: 100px\"\r\n                  :src=\"scope.row.foodPic\"\r\n                  :preview-src-list=\"[scope.row.foodPic]\"\r\n                >\r\n                </el-image>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"foodName\" label=\"食品名\"> </el-table-column>\r\n          <el-table-column prop=\"price\" label=\"价钱\"> </el-table-column>\r\n          <el-table-column prop=\"fcount\" label=\"数量\">\r\n            <template slot-scope=\"scope\">\r\n\t\t\t\t\t\t<div style=\"display: flex;align-items: center;justify-content: \r\n                        center;\">\r\n\t\t\t\t\t\t\t<el-button size=\"small\" @click=\"decrement(scope.$index)\" \r\n                            :disabled=\"scope.row.fcount <= 1\">-</el-button>\r\n\t\t\t\t\t\t\t<div style=\"width: 50px;\">{{scope.row.fcount}}</div>\r\n\t\t\t\t\t\t\t<el-button size=\"small\" type=\"primary\" \r\n                            @click=\"increment(scope.$index)\">+</el-button>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</template>\r\n          </el-table-column>\r\n          <el-table-column fixed=\"right\" label=\"操作\" width=\"100\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                slot=\"reference\"\r\n                type=\"danger\"\r\n                size=\"small\"\r\n                @click=\"handleDelete(scope.row.fid)\"\r\n                >删除</el-button\r\n              >\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <div style=\"margin-top: 20px; overflow: hidden\">\r\n          <h2 style=\"text-align: center\">总计：{{ totalPrice | showPrice }}</h2>\r\n          <el-button\r\n            type=\"primary\"\r\n            style=\"\r\n              float: right;\r\n              margin-right: 50px;\r\n              width: 200px;\r\n              height: 50px;\r\n              font-size: 25px;\r\n            \"\r\n            @click=\"finshShopping\"\r\n            >完成购买</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\nexport default {\r\n  name: \"Cart\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      uid: \"\",\r\n      multipleSelection: [],\r\n      address: \"\",\r\n      foodName: \"\",\r\n      foodPic: \"\",\r\n      nickName: \"\",\r\n      sessionFood:\"\",\r\n      cout:0\r\n    };\r\n  },\r\n  created() {\r\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\r\n    this.uid = this.session.username;\r\n    this.address = this.session.address;\r\n    // this.fid = this.sessionFood[0].fid;\r\n\r\n    // console.log(this.uid+this.address);\r\n    // console.log(this.session.username);\r\n    this.showCartInfo();\r\n    this.load();\r\n  },\r\n  methods: {\r\n     load(){\r\n            request.get(\"/user/selectBalance\",{\r\n        params:{\r\n            username:this.uid\r\n        }\r\n    }).then((res)=>{\r\n        this.balance = res.data[0].balance;\r\n        \r\n        // console.log(res);\r\n\r\n    })\r\n     },\r\n    finshShopping() {\r\n      if(this.balance >=this.totalPrice){\r\n              let oid = this.cout + \"o\" + this.uid ;\r\n      this.cout = this.cout+1;\r\n      // console.log(oid);\r\n      request\r\n        .get(\"/cart/finishShopping\", {\r\n          params: {\r\n            oid: oid,\r\n            username: this.uid,\r\n            address: this.address,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          request.get(\"/cart/deleteCartInfoByUid\",{\r\n            params:{\r\n              uid:this.uid\r\n            }\r\n          }).then((res)=>{\r\n            this.showCartInfo();\r\n            // console.log(res);\r\n          })\r\n          // console.log(res);\r\n          this.$message({\r\n            message: \"下单成功请等待配送\",\r\n            type: \"success\",\r\n          });\r\n\r\n        });\r\n      }else{\r\n        this.$message({\r\n            type: \"error\",\r\n            message: \"余额不足\",\r\n          });\r\n      }\r\n\r\n    },\r\n    showCartInfo() {\r\n      request\r\n        .get(\"/cart/selectCartInfo\", {\r\n          params: {\r\n            uid: this.uid,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          // sessionStorage.setItem(\"food\", JSON.stringify(res.data));\r\n          this.tableData = res.data;\r\n          // console.log(this.foodInfo);\r\n          // console.log(this.tableData);\r\n          console.log(res);\r\n        });\r\n    },\r\n    increment(index) {\r\n      //加\r\n      // console.log(this.tableData);\r\n      // console.log(index);\r\n      this.tableData[index].fcount++;\r\n      console.log(this.tableData[index].fcount);\r\n      request.get(\"/cart/updateFoodNum\",{\r\n        params:{\r\n          fid:this.tableData[index].fid,\r\n          fcount:this.tabledata[index].fcount\r\n        }\r\n      }).then((res)=>{\r\n        console.log(res);\r\n      })\r\n    },\r\n    decrement(index) {\r\n      //减\r\n      this.tableData[index].fcount--;\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = val;\r\n    },\r\n    handleDelete(fid) {\r\n      request.get(\"/cart/deleteCartInfo\",{\r\n        params:{\r\n          fid:fid\r\n        }\r\n      }).then((res) => {\r\n        if (res.code == \"0\") {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功\",\r\n          });\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg,\r\n          });\r\n        }\r\n        this.showCartInfo();\r\n      });\r\n    },\r\n  },\r\n  computed: {\r\n    totalPrice() {\r\n      //计算总价\r\n      let total = 0;\r\n      for (let i in this.tableData) {\r\n        total += this.tableData[i].price * this.tableData[i].fcount;\r\n      }\r\n      return total;\r\n    },\r\n  },\r\n  filters: {\r\n    //过滤器\r\n    // 拼接价格/总价\r\n    showPrice(price) {\r\n      return \"￥\" + price.toFixed(2);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}