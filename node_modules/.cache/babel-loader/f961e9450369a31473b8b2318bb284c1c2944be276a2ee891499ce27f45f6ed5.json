{"ast":null,"code":"import request from \"@/utils/request.js\";\nexport default {\n  name: \"Cart\",\n  data() {\n    return {\n      tableData: [{\n        count: 1\n      }],\n      uid: '',\n      count: 1,\n      multipleSelection: []\n    };\n  },\n  created() {\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\n    this.uid = this.session.username;\n    // console.log(this.session.username);\n    this.showCartInfo();\n  },\n  methods: {\n    showCartInfo() {\n      request.get(\"/cart/selectCartInfo\", {\n        params: {\n          uid: this.uid\n        }\n      }).then(res => {\n        // sessionStorage.setItem(\"food\", JSON.stringify(res.data));\n        this.tableData = res.data;\n        // console.log(this.foodInfo);\n        console.log(this.tableData);\n      });\n    },\n    toggleSelection(rows) {\n      if (rows) {\n        rows.forEach(row => {\n          this.$refs.multipleTable.toggleRowSelection(row);\n        });\n      } else {\n        this.$refs.multipleTable.clearSelection();\n      }\n    },\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    }\n  }\n};","map":{"version":3,"mappings":"AA8DA;AACA;EACAA;EACAC;IACA;MACAC;QAAAC;MAAA;MACAC;MACAD;MACAE;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;EACA;EACAC;IACAC;MACAC;QACAC;UACAN;QACA;MACA;QACA;QACA;QACA;QACAO;MACA;IACA;IACAC;MACA;QACAC;UACA;QACA;MACA;QACA;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","tableData","count","uid","multipleSelection","created","methods","showCartInfo","request","params","console","toggleSelection","rows","handleSelectionChange"],"sourceRoot":"src/views","sources":["Cart.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 购物车主界面 -->\r\n    <div>\r\n      <div>\r\n        <h2\r\n          style=\"\r\n            margin: 15px 20px;\r\n            color: #409eff;\r\n            border-bottom: 5px solid gray;\r\n            padding-bottom: 8px;\r\n          \"\r\n        >\r\n          购物车\r\n        </h2>\r\n      </div>\r\n      <div>\r\n        <el-table\r\n          ref=\"multipleTable\"\r\n          :data=\"tableData\"\r\n          tooltip-effect=\"dark\"\r\n          style=\"width: 100%\"\r\n          @selection-change=\"handleSelectionChange\"\r\n        >\r\n          <el-table-column type=\"selection\" width=\"55\"> </el-table-column>\r\n          <el-table-column prop=\"foodPic\" label=\"食品图片\">\r\n            <template slot-scope=\"scope\">\r\n              <div class=\"demo-image__preview\">\r\n                <el-image\r\n                  style=\"width: 100px; height: 100px\"\r\n                  :src=\"scope.row.foodPic\"\r\n                  :preview-src-list=\"[scope.row.foodPic]\"\r\n                >\r\n                </el-image>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"count\" label=\"数量\"></el-table-column>\r\n          <el-table-column prop=\"foodName\" label=\"食品名\"> </el-table-column>\r\n          <el-table-column prop=\"price\" label=\"价钱\"> </el-table-column>\r\n                <el-table-column fixed=\"right\" label=\"操作\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-popconfirm\r\n            title=\"确认删除吗\"\r\n            @confirm=\"handleDelete(scope.row.id)\"\r\n          >\r\n            <el-button slot=\"reference\" type=\"danger\" size=\"small\"\r\n              >删除</el-button\r\n            >\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n        </el-table>\r\n        <div style=\"margin-top: 20px\">\r\n          <el-button @click=\"toggleSelection()\">取消选择</el-button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\nexport default {\r\n  name: \"Cart\",\r\n  data() {\r\n    return {\r\n      tableData: [{count:1}],\r\n      uid:'',\r\n      count:1,\r\n      multipleSelection: []\r\n    };\r\n  },\r\