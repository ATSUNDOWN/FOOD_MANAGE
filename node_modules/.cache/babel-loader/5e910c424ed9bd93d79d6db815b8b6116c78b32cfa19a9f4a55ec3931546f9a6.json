{"ast":null,"code":"import request from \"@/utils/request.js\";\nexport default {\n  name: \"Cart\",\n  data() {\n    return {\n      tableData: [],\n      uid: \"\",\n      multipleSelection: []\n    };\n  },\n  created() {\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\n    this.uid = this.session.username;\n    // console.log(this.session.username);\n    this.showCartInfo();\n  },\n  methods: {\n    finshShopping() {\n      oid = \"o\" + username;\n      request.get(\"/cart/finishShopping\", {\n        params: {\n          oid: this.oid,\n          username: this.username,\n          address: this.address\n        }\n      }).then(res => {\n        this.$message({\n          message: '下单成功请等待配送',\n          type: 'success'\n        });\n      });\n    },\n    showCartInfo() {\n      request.get(\"/cart/selectCartInfo\", {\n        params: {\n          uid: this.uid\n        }\n      }).then(res => {\n        // sessionStorage.setItem(\"food\", JSON.stringify(res.data));\n        this.tableData = res.data;\n        // console.log(this.foodInfo);\n        // console.log(this.tableData);\n      });\n    },\n\n    increment(index) {\n      //加\n      this.tableData[index].fcount++;\n    },\n    decrement(index) {\n      //减\n      this.tableData[index].fcount--;\n    },\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n    handleDelete(id) {\n      // console.log(id);\n      request.delete(\"/cart/\" + id).then(res => {\n        if (res.code == \"0\") {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n        this.showCartInfo(); //重新加载\n      });\n    }\n  },\n\n  computed: {\n    totalPrice() {\n      //计算总价\n      let total = 0;\n      for (let i in this.tableData) {\n        total += this.tableData[i].price * this.tableData[i].fcount;\n      }\n      return total;\n    }\n  },\n  filters: {\n    //过滤器\n    // 拼接价格/总价\n    showPrice(price) {\n      return \"￥\" + price.toFixed(2);\n    }\n  }\n};","map":{"version":3,"mappings":"AAiGA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;EACA;EACAC;IACAC;MACAC;MACAC;QACAC;UACAF;UACAG;UACAC;QACA;MACA;QACA;UACAC;UACAC;QACA;MACA;IACA;IACAC;MACAN,QACAO;QACAN;UACAP;QACA;MACA,GACAc;QACA;QACA;QACA;QACA;MACA;IACA;;IACAC;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;MACAZ;QACA;UACA;YACAK;YACAD;UACA;QACA;UACA;YACAC;YACAD;UACA;QACA;QACA;MACA;IACA;EACA;;EACAS;IACAC;MACA;MACA;MACA;QACAC;MACA;MACA;IACA;EACA;EACAC;IACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","tableData","uid","multipleSelection","created","methods","finshShopping","oid","request","params","username","address","message","type","showCartInfo","get","then","increment","decrement","handleSelectionChange","handleDelete","computed","totalPrice","total","filters","showPrice"],"sourceRoot":"src/views","sources":["Cart.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- 购物车主界面 -->\r\n    <div>\r\n      <div>\r\n        <h2\r\n          style=\"\r\n            margin: 15px 20px;\r\n            color: #409eff;\r\n            border-bottom: 5px solid gray;\r\n            padding-bottom: 8px;\r\n          \"\r\n        >\r\n          购物车\r\n        </h2>\r\n      </div>\r\n      <div>\r\n        <el-table\r\n          ref=\"multipleTable\"\r\n          :data=\"tableData\"\r\n          tooltip-effect=\"dark\"\r\n          style=\"width: 100%\"\r\n          @selection-change=\"handleSelectionChange\"\r\n        >\r\n          <el-table-column prop=\"foodPic\" label=\"食品图片\">\r\n            <template slot-scope=\"scope\">\r\n              <div class=\"demo-image__preview\">\r\n                <el-image\r\n                  style=\"width: 100px; height: 100px\"\r\n                  :src=\"scope.row.foodPic\"\r\n                  :preview-src-list=\"[scope.row.foodPic]\"\r\n                >\r\n                </el-image>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"foodName\" label=\"食品名\"> </el-table-column>\r\n          <el-table-column prop=\"price\" label=\"价钱\"> </el-table-column>\r\n          <el-table-column prop=\"fcount\" label=\"数量\">\r\n            <template slot-scope=\"scope\">\r\n              <div\r\n                style=\"\r\n                  display: flex;\r\n                  align-items: center;\r\n                  justify-content: center;\r\n                \"\r\n              >\r\n                <el-button\r\n                  size=\"small\"\r\n                  @click=\"decrement(scope.$index)\"\r\n                  :disabled=\"scope.row.fcount <= 1\"\r\n                  >-</el-button\r\n                >\r\n                <div style=\"width: 50px\">{{ scope.row.fcount }}</div>\r\n                <el-button\r\n                  size=\"small\"\r\n                  type=\"primary\"\r\n                  @click=\"increment(scope.$index)\"\r\n                  >+</el-button\r\n                >\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column fixed=\"right\" label=\"操作\" width=\"100\">\r\n            <template slot-scope=\"scope\">\r\n              <el-popconfirm\r\n                title=\"确认删除吗\"\r\n                @confirm=\"handleDelete(scope.row.id)\"\r\n              >\r\n                <el-button slot=\"reference\" type=\"danger\" size=\"small\"\r\n                  >删除</el-button\r\n                >\r\n              </el-popconfirm>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <div style=\"margin-top: 20px; overflow: hidden\">\r\n          <h2 style=\"text-align: center\">总计：{{ totalPrice | showPrice }}</h2>\r\n          <el-button\r\n            type=\"primary\"\r\n            style=\"\r\n              float: right;\r\n              margin-right: 50px;\r\n              width: 200px;\r\n              height: 50px;\r\n              font-size: 25px;\r\n            \"\r\n            @click=\"finsihShop\"\r\n            >完成购买</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\nexport default {\r\n  name: \"Cart\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      uid: \"\",\r\n      multipleSelection: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\r\n    this.uid = this.session.username;\r\n    // console.log(this.session.username);\r\n    this.showCartInfo();\r\n  },\r\n  methods: {\r\n    finshShopping() {\r\n      oid = \"o\" + username;\r\n      request.get(\"/cart/finishShopping\", {\r\n        params: {\r\n          oid: this.oid,\r\n          username: this.username,\r\n          address: this.address,\r\n        },\r\n      }).then((res) =>{\r\n        this.$message({\r\n          message: '下单成功请等待配送',\r\n          type: 'success'\r\n        });\r\n      })\r\n    },\r\n    showCartInfo() {\r\n      request\r\n        .get(\"/cart/selectCartInfo\", {\r\n          params: {\r\n            uid: this.uid,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          // sessionStorage.setItem(\"food\", JSON.stringify(res.data));\r\n          this.tableData = res.data;\r\n          // console.log(this.foodInfo);\r\n          // console.log(this.tableData);\r\n        });\r\n    },\r\n    increment(index) {\r\n      //加\r\n      this.tableData[index].fcount++;\r\n    },\r\n    decrement(index) {\r\n      //减\r\n      this.tableData[index].fcount--;\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = val;\r\n    },\r\n    handleDelete(id) {\r\n      // console.log(id);\r\n      request.delete(\"/cart/\" + id).then((res) => {\r\n        if (res.code == \"0\") {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功\",\r\n          });\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg,\r\n          });\r\n        }\r\n        this.showCartInfo(); //重新加载\r\n      });\r\n    },\r\n  },\r\n  computed: {\r\n    totalPrice() {\r\n      //计算总价\r\n      let total = 0;\r\n      for (let i in this.tableData) {\r\n        total += this.tableData[i].price * this.tableData[i].fcount;\r\n      }\r\n      return total;\r\n    },\r\n  },\r\n  filters: {\r\n    //过滤器\r\n    // 拼接价格/总价\r\n    showPrice(price) {\r\n      return \"￥\" + price.toFixed(2);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}