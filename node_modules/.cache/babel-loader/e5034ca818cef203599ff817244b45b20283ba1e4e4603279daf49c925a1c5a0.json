{"ast":null,"code":"import request from \"@/utils/request.js\";\nexport default {\n  name: \"Person\",\n  components: {},\n  data() {\n    return {\n      form: {},\n      nickName: \"\",\n      password: \"\",\n      sex: \"\",\n      age: \"\",\n      address: \"\",\n      search: \"\"\n    };\n  },\n  created() {\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\n    this.nickName = this.session.nickName;\n    this.password = this.session.password;\n    this.sex = this.session.sex;\n    this.age = this.session.age;\n    this.address = this.session.address;\n  },\n  methods: {\n    update() {\n      request.put(\"/user\", this.form).then(res => {\n        console.log(res);\n        if (res.code === \"0\") {\n          this.$message({\n            type: \"success\",\n            message: \"更新\"\n          });\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form));\n          this.$$emit(\"user\");\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAgHA;AACA;EACAA;EACAC;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;IACA;IACA;EACA;EACAC;IACAC;MACAC;QACAC;QACA;UACA;YACAC;YACAC;UACA;UACAC;UACA;QACA;UACA;YACAF;YACAC;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","components","data","form","nickName","password","sex","age","address","search","created","methods","update","request","console","type","message","sessionStorage"],"sourceRoot":"src/views","sources":["Person.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div\r\n      style=\"\r\n        height: 80px;\r\n        line-height: 50px;\r\n        border-bottom: 1px solid #ccc;\r\n        display: flex;\r\n      \"\r\n    >\r\n      <div>\r\n        <img\r\n          src=\"@/assets/logo.jpg\"\r\n          style=\"width: 120px; height: 100px; margin-left: 20px\"\r\n        />\r\n      </div>\r\n      <div style=\"flex: 1\">\r\n        <!-- 搜索区域 -->\r\n        <div style=\"margin: 5px 0; text-align: center\">\r\n          <el-input\r\n            v-model=\"search\"\r\n            placeholder=\"请输入关键字\"\r\n            style=\"width: 35%\"\r\n          ></el-input>\r\n          <el-button type=\"primary\" style=\"margin-left: 5px\">查询</el-button>\r\n        </div>\r\n      </div>\r\n      <!-- 下滑栏 -->\r\n      <div style=\"width: 100px\">\r\n        <el-dropdown>\r\n          <span class=\"el-dropdown-link\">\r\n            {{ nickName }}<i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n          </span>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item @click.native=\"$router.push('/person')\"\r\n              >个人信息</el-dropdown-item\r\n            >\r\n            <el-dropdown-item @click.native=\"$router.push('/person')\"\r\n              >购物车</el-dropdown-item\r\n            >\r\n            <el-dropdown-item @click.native=\"$router.push('/login')\"\r\n              >退出系统</el-dropdown-item\r\n            >\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n    </div>\r\n    <!-- 中部导航栏 -->\r\n    <el-menu class=\"el-menu-demo\" mode=\"horizontal\" background-color=\"#409eff\">\r\n      <!-- <el-menu-item\r\n        v-for=\"(item, index) in cuisines\"\r\n        :key=\"index\"\r\n        style=\"width: 100px; color: white\"\r\n        >{{ item.cuisine }}</el-menu-item\r\n      > -->\r\n      <el-menu-item\r\n        style=\"color: white; margin-left: 100px\"\r\n        @click=\"$router.push('/home')\"\r\n        >主页\r\n      </el-menu-item>\r\n      <el-menu-item style=\"color: white\">闽菜 </el-menu-item>\r\n      <el-menu-item style=\"color: white\">粤菜 </el-menu-item>\r\n      <el-menu-item style=\"color: white\">鲁菜 </el-menu-item>\r\n      <el-menu-item style=\"color: white\">浙菜 </el-menu-item>\r\n      <el-menu-item style=\"color: white\">苏菜 </el-menu-item>\r\n    </el-menu>\r\n    <div style=\"display: flex\">\r\n      <div\r\n        style=\"\r\n          width: 800px;\r\n          height: 800px;\r\n          margin: 50px 0 0 25%;\r\n          border-radius: 20px;\r\n        \"\r\n      >\r\n        <div\r\n          style=\"\r\n            width: 500px;\r\n            height: 500px;\r\n            margin-left: 150px;\r\n            margin-top: 100px;\r\n          \"\r\n        >\r\n          <div style=\"margin-left:50px;\">\r\n            <div>\r\n              <h3 style=\"float: left;\">昵称:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ nickName }}</div>\r\n            </div>\r\n            <div style=\"margin-top:50px\">\r\n              <h3 style=\"float: left;\">年龄:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ age }}</div>\r\n            </div>\r\n            <div style=\"margin-top:50px\">\r\n              <h3 style=\"float: left;padding-top:10px\">密码:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ password }}</div>\r\n            </div>\r\n            <div style=\"margin-top:50px\">\r\n              <h3 style=\"float: left;padding-top:10px\">性别:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ sex }}</div>\r\n            </div>\r\n            <div style=\"margin-top:50px\">\r\n              <h3 style=\"float: left;padding-top:10px\">地址:</h3>\r\n              <div style=\"font-size:18px;margin-left:75px;border:2px solid whitesmoke;border-radius:5px;padding:10px 30%\">{{ address }}</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\nexport default {\r\n  name: \"Person\",\r\n  components: {},\r\n  data() {\r\n    return {\r\n      form: {},\r\n      nickName: \"\",\r\n      password: \"\",\r\n      sex: \"\",\r\n      age: \"\",\r\n      address: \"\",\r\n      search: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\r\n    this.nickName = this.session.nickName;\r\n    this.password = this.session.password;\r\n    this.sex = this.session.sex;\r\n    this.age = this.session.age;\r\n    this.address = this.session.address;\r\n  },\r\n  methods: {\r\n    update() {\r\n      request.put(\"/user\", this.form).then((res) => {\r\n        console.log(res);\r\n        if (res.code === \"0\") {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"更新\",\r\n          });\r\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form));\r\n          this.$$emit(\"user\");\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg,\r\n          });\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}