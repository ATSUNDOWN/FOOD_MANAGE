{"ast":null,"code":"import request from \"@/utils/request.js\";\nexport default {\n  name: \"Order\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      search: \"\",\n      currentPage: 1,\n      pageSize: 10,\n      oid: ''\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/ordering\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n        // console.log(this.tableData);\n      });\n    },\n\n    handleClose(done) {\n      this.$confirm(\"确认关闭？\").then(_ => {\n        done();\n      }).catch(_ => {});\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"mappings":"AAqDA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACAC,QACAC;QACAC;UACAC;UACAR;UACAF;QACA;MACA,GACAW;QACAC;QACA;QACA;QACA;MACA;IACA;;IACAC;MACA,uBACAF;QACAG;MACA,GACAC;IACA;IACAC;MACA;MACA;IACA;IACAC;MAAA;MACA;MACA;IACA;IACAC;MAAA;MACA;MACA;IACA;EACA;AACA","names":["name","data","tableData","total","search","currentPage","pageSize","oid","created","methods","load","request","get","params","pageNum","then","console","handleClose","done","catch","handleEdit","handleSizeChange","handleCurrentChange"],"sourceRoot":"src/views","sources":["Order.vue"],"sourcesContent":["<template>\r\n  <div>\r\n     <!-- 功能区域 -->\r\n    <div style=\"margin: 10px 0\">\r\n      <!-- <el-button type=\"primary\">导入</el-button>\r\n      <el-button type=\"primary\">导出</el-button> -->\r\n    </div>\r\n    <!-- 搜索区域 -->\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input\r\n        v-model=\"search\"\r\n        placeholder=\"请输入关键字\"\r\n        style=\"width: 20%\"\r\n      ></el-input>\r\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\"\r\n        >查询</el-button\r\n      >\r\n    </div>\r\n    <el-table :data=\"tableData\" border stripe style=\"width: 100%\">\r\n      <el-table-column prop=\"id\" label=\"ID\" sortable> </el-table-column>\r\n      <el-table-column prop=\"oid\" label=\"订单号\"> </el-table-column>\r\n      <el-table-column prop=\"username\" label=\"用户名\"> </el-table-column>\r\n      <el-table-column prop=\"address\" label=\"地址\"> </el-table-column>\r\n      <el-table-column prop=\"status\" label=\"状态\"> </el-table-column>\r\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            @click=\"handleEdit(scope.row)\"\r\n            type=\"button\"\r\n            size=\"small\"\r\n            style=\"margin-bottom: 5px\"\r\n            >编辑</el-button\r\n          >\r\n          <el-popconfirm title=\"确认删除吗\" @confirm=\"handleDelete(scope.row.id)\">\r\n            <el-button slot=\"reference\" type=\"danger\" size=\"small\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        :page-sizes=\"[5, 10, 20]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\n\r\nexport default {\r\n  name: \"Order\",\r\n  data(){\r\n    return{\r\n        tableData:[],\r\n        total:0,\r\n        search: \"\",\r\n        currentPage: 1,\r\n        pageSize: 10,\r\n        oid:''\r\n    }\r\n  },\r\n  created(){\r\n    this.load();\r\n  },\r\n  methods:{\r\n    load() {\r\n      request\r\n        .get(\"/ordering\", {\r\n          params: {\r\n            pageNum: this.currentPage,\r\n            pageSize: this.pageSize,\r\n            search: this.search,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          console.log(res);\r\n          this.tableData = res.data.records;\r\n          this.total = res.data.total;\r\n          // console.log(this.tableData);\r\n        });\r\n    },\r\n    handleClose(done) {\r\n      this.$confirm(\"确认关闭？\")\r\n        .then((_) => {\r\n          done();\r\n        })\r\n        .catch((_) => {});\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row));\r\n      this.dialogVisible = true\r\n    },\r\n    handleSizeChange(pageSize) {  //改变当前每页的个数触发\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) { //改变当前页码触发\r\n      this.currentPage = pageNum\r\n      this.load()\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}