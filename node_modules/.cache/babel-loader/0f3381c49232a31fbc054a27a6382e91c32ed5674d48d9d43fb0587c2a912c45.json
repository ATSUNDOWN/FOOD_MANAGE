{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request.js\";\nexport default {\n  name: \"Header\",\n  data() {\n    return {\n      nickName: \"\",\n      session: \"\",\n      sessionData: \"\",\n      search: \"\",\n      pattern: [],\n      foodInfo: [],\n      oneFood: [],\n      index: 0,\n      id: 0\n    };\n  },\n  created() {\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\n    this.nickName = this.session.nickName;\n    this.load();\n    this.showIntro();\n  },\n  methods: {\n    toDetail(id) {\n      request.get(\"/food/getFoodInfoById\", {\n        params: {\n          id: id\n        }\n      }).then(res => {\n        // console.log(res.data.foodName);\n        // console.log(res.data);\n        sessionStorage.setItem(\"food\", JSON.stringify(res.data));\n        this.$router.push('/detail');\n        // this.oneFood = \n        // console.log(res.data.id);\n      });\n    },\n\n    keyWordCheck() {\n      request.get(\"/food/keyWordCheck\", {\n        params: {\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res.data);\n        this.foodInfo = res.data;\n        console.log(this.foodInfo);\n      });\n    },\n    showIntro() {\n      request.get(\"/food/getAllFoodInfo\").then(res => {\n        // sessionStorage.setItem(\"food\", JSON.stringify(res.data));\n        this.foodInfo = res.data;\n        // console.log(this.foodInfo);\n      });\n    },\n\n    load() {\n      request.get(\"/food/selectPatternInfo\").then(res => {\n        this.pattern = res.data;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AA4MA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;EACA;EACAC;IACAC;MACAC;QACAC;UACAL;QACA;MACA;QACA;QACA;QACAM;QACA;QACA;QACA;MACA;IAIA;;IACAC;MACAH,QACAI;QACAH;UACAV;QACA;MACA,GACAc;QACAC;QACA;QACAA;MACA;IACA;IACAC;MACAP;QACA;QACA;QACA;MACA;IACA;;IACAQ;MACAR;QACA;MACA;IACA;EACA;AACA","names":["name","data","nickName","session","sessionData","search","pattern","foodInfo","oneFood","index","id","created","methods","toDetail","request","params","sessionStorage","keyWordCheck","get","then","console","showIntro","load"],"sourceRoot":"src/views","sources":["Home.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div\r\n      style=\"\r\n        height: 80px;\r\n        line-height: 50px;\r\n        border-bottom: 1px solid #ccc;\r\n        display: flex;\r\n      \"\r\n    >\r\n      <div>\r\n        <img\r\n          src=\"@/assets/logo.jpg\"\r\n          style=\"width: 120px; height: 100px; margin-left: 20px\"\r\n        />\r\n      </div>\r\n      <div style=\"flex: 1\">\r\n        <!-- 搜索区域 -->\r\n        <div style=\"margin: 5px 0; text-align: center\">\r\n          <el-input\r\n            v-model=\"search\"\r\n            placeholder=\"请输入关键字\"\r\n            style=\"width: 35%\"\r\n          ></el-input>\r\n          <el-button\r\n            type=\"primary\"\r\n            style=\"margin-left: 5px\"\r\n            @click=\"keyWordCheck\"\r\n            >查询</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n      <!-- 下滑栏 -->\r\n      <div style=\"width: 100px\">\r\n        <el-dropdown>\r\n          <span class=\"el-dropdown-link\">\r\n            {{ nickName }}<i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n          </span>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item @click.native=\"$router.push('/person')\"\r\n              >个人信息</el-dropdown-item\r\n            >\r\n            <el-dropdown-item @click.native=\"$router.push('/cart')\"\r\n              >购物车</el-dropdown-item\r\n            >\r\n            <el-dropdown-item @click.native=\"$router.push('/favorites')\"\r\n              >收藏夹</el-dropdown-item\r\n            >\r\n            <el-dropdown-item @click.native=\"$router.push('/myorder')\"\r\n              >我的订单</el-dropdown-item\r\n            >\r\n            <el-dropdown-item @click.native=\"$router.push('/wallet')\"\r\n              >我的钱包</el-dropdown-item\r\n            >\r\n            <el-dropdown-item @click.native=\"$router.push('/login')\"\r\n              >退出系统</el-dropdown-item\r\n            >\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n    </div>\r\n    <!-- 中部导航栏 -->\r\n    <el-menu class=\"el-menu-demo\" mode=\"horizontal\" background-color=\"#409eff\">\r\n      <!-- <el-menu-item\r\n        v-for=\"(item, index) in cuisines\"\r\n        :key=\"index\"\r\n        style=\"width: 100px; color: white\"\r\n        >{{ item.cuisine }}</el-menu-item\r\n      > -->\r\n      <el-menu-item\r\n        style=\"color: white; margin-left: 100px\"\r\n        @click=\"$router.push('/home')\"\r\n        >主页\r\n      </el-menu-item>\r\n      <el-menu-item style=\"color: white\" @click.native=\"$router.push('/cart')\">购物车 </el-menu-item>\r\n      <el-menu-item style=\"color: white\" @click.native=\"$router.push('/favorites')\">收藏夹</el-menu-item>\r\n      <el-menu-item style=\"color: white\" @click.native=\"$router.push('/wallet')\">我的钱包</el-menu-item>\r\n      <el-menu-item style=\"color: white\" @click.native=\"$router.push('/myorder')\">我的订单</el-menu-item>\r\n      <el-menu-item style=\"color: white\" @click.native=\"$router.push('/comment')\">讨论区</el-menu-item>\r\n    </el-menu>\r\n    <el-container>\r\n      <!-- 中部侧边栏 -->\r\n      <el-aside width=\"200px\">\r\n        <el-row class=\"tac\" style=\"width: 200px\">\r\n          <el-col :span=\"20\">\r\n            <el-menu\r\n              default-active=\"2\"\r\n              class=\"el-menu-vertical-demo\"\r\n              v-for=\"(item, index) in pattern\"\r\n              :key=\"index\"\r\n              style=\"margin: 18px 0 0 0px; text-align: center; font-size: 15px\"\r\n            >\r\n              {{ item.pattern }}\r\n            </el-menu>\r\n          </el-col>\r\n        </el-row>\r\n      </el-aside>\r\n      <!-- 轮播图 -->\r\n      <el-main>\r\n        <el-carousel :interval=\"5000\" arrow=\"always\" style=\"flex: 1\">\r\n          <el-carousel-item\r\n            v-for=\"item in 3\"\r\n            :key=\"item\"\r\n            style=\"margin-left: 300px\"\r\n          >\r\n          <img src=\"@\" alt=\"\">\r\n          </el-carousel-item>\r\n        </el-carousel>\r\n      </el-main>\r\n    </el-container>\r\n    <!-- 菜品展览 -->\r\n    <div style=\"border-top: 5px solid #409eff; padding-top: 20px\">\r\n      <h1\r\n        style=\"\r\n          margin-bottom: 20px;\r\n          color: #409eff;\r\n          border-left: 5px solid #409e;\r\n          border-top: 10px solid #409e;\r\n          padding-left: 10px;\r\n        \"\r\n      >\r\n        菜品展示\r\n      </h1>\r\n      <div\r\n        style=\"\r\n          display: flex;\r\n          gap: 5px;\r\n          flex-direction: row;\r\n          flex-wrap: wrap;\r\n          justify-content: space-around;\r\n        \"\r\n       \r\n      >\r\n        <div\r\n          style=\"\r\n            height: 250px;\r\n            width: 300px;\r\n            background: whitesmoke;\r\n            margin-bottom: 20px;\r\n          \"\r\n          v-for=\"(item, index) in foodInfo\"\r\n            @click=\"toDetail(item.id)\"\r\n          :key=\"index\"\r\n        >\r\n          <div class=\"picture\" style=\"height: 200px; width: 300px\">\r\n            <el-image\r\n              :src=\"item.foodPic\"\r\n              style=\"height: 200px; width: 300px\"\r\n            ></el-image>\r\n          </div>\r\n          <div class=\"intro\">\r\n            <div\r\n              style=\"\r\n                width: 150px;\r\n                height: 50px;\r\n                float: left;\r\n                overflow: hidden;\r\n                padding-top: 15px;\r\n                text-align: center;\r\n                background: #409e;\r\n              \"\r\n            >\r\n              <span style=\"color: white\">{{ item.foodName }}</span>\r\n            </div>\r\n            <div\r\n              style=\"\r\n                width: 150px;\r\n                height: 50px;\r\n                overflow: hidden;\r\n                padding-top: 15px;\r\n                text-align: center;\r\n                background: #409eff;\r\n              \"\r\n            >\r\n              <span style=\"color: white\">{{ item.pattern }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div\r\n        style=\"\r\n          border-bottom: 10px solid #409e;\r\n          border-right: 5px solid #409e;\r\n          float: right;\r\n          margin-bottom: 10px;\r\n        \"\r\n      >\r\n        <h4 style=\"color: grey; margin-right: 20px; margin-bottom: 5px\">\r\n          已经到末尾啦↑\r\n        </h4>\r\n      </div>\r\n    </div>\r\n    <div\r\n      class=\"end\"\r\n      style=\"width: 100%; height: 25px; background: #419eff; overflow: hidden\"\r\n    >\r\n      <h5 style=\"color: grey; text-align: center; line-height: 25px\">\r\n        Copyright ©2023丨graduation design for QX丨YSJ\r\n      </h5>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request.js\";\r\n\r\nexport default {\r\n  name: \"Header\",\r\n  data() {\r\n    return {\r\n      nickName: \"\",\r\n      session: \"\",\r\n      sessionData: \"\",\r\n      search: \"\",\r\n      pattern: [],\r\n      foodInfo: [],\r\n      oneFood:[],\r\n      index: 0,\r\n      id:0\r\n    };\r\n  },\r\n  created() {\r\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\r\n    this.nickName = this.session.nickName;\r\n    this.load();\r\n    this.showIntro();\r\n  },\r\n  methods: {\r\n    toDetail(id){\r\n      request.get(\"/food/getFoodInfoById\",{\r\n        params:{\r\n          id:id\r\n        }\r\n      }).then((res) =>{\r\n        // console.log(res.data.foodName);\r\n        // console.log(res.data);\r\n        sessionStorage.setItem(\"food\", JSON.stringify(res.data));\r\n        this.$router.push('/detail')\r\n        // this.oneFood = \r\n        // console.log(res.data.id);\r\n      })\r\n\r\n\r\n\r\n    },\r\n    keyWordCheck() {\r\n      request\r\n        .get(\"/food/keyWordCheck\", {\r\n          params: {\r\n            search: this.search,\r\n          },\r\n        })\r\n        .then((res) => {\r\n          console.log(res.data);\r\n          this.foodInfo = res.data;\r\n          console.log(this.foodInfo);\r\n        });\r\n    },\r\n    showIntro() {\r\n      request.get(\"/food/getAllFoodInfo\").then((res) => {\r\n        // sessionStorage.setItem(\"food\", JSON.stringify(res.data));\r\n        this.foodInfo = res.data;       \r\n        // console.log(this.foodInfo);\r\n      });\r\n    },\r\n    load() {\r\n      request.get(\"/food/selectPatternInfo\").then((res) => {\r\n        this.pattern = res.data;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.el-row {\r\n  margin-bottom: 20px;\r\n}\r\n.el-col {\r\n  border-radius: 4px;\r\n}\r\n.bg-purple-dark {\r\n  background: #99a9bf;\r\n}\r\n.bg-purple {\r\n  background: #d3dce6;\r\n}\r\n.bg-purple-light {\r\n  background: #e5e9f2;\r\n}\r\n.grid-content {\r\n  border-radius: 4px;\r\n  min-height: 36px;\r\n}\r\n.row-bg {\r\n  padding: 10px 0;\r\n  background-color: #f9fafc;\r\n}\r\n.el-dropdown-link {\r\n  cursor: pointer;\r\n  color: #409eff;\r\n}\r\n.el-icon-arrow-down {\r\n  font-size: 12px;\r\n}\r\n.el-carousel__item h3 {\r\n  color: #475669;\r\n  font-size: 18px;\r\n  opacity: 0.75;\r\n  line-height: 300px;\r\n  margin: 0;\r\n}\r\n.el-carousel__item:nth-child(2n) {\r\n  background: url(\"@/assets/food1.jpg\") no-repeat;\r\n  background-color: #d3dce6;\r\n}\r\n\r\n.el-carousel__item:nth-child(2n + 1) {\r\n  background: url(\"@/assets/food2.jpg\") no-repeat;\r\n  background-color: #d3dce6;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}