nu>\r\n          </el-col>\r\n        </el-row>\r\n      </el-aside>\r\n      <!-- 轮播图 -->\r\n      <el-main>\r\n        <el-carousel :interval=\"5000\" arrow=\"always\" style=\"flex: 1\">\r\n          <el-carousel-item v-for=\"item in 4\" :key=\"item\">\r\n            <h3>{{ item }}</h3>\r\n          </el-carousel-item>\r\n        </el-carousel>\r\n      </el-main>\r\n    </el-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Header\",\r\n  data() {\r\n    return {\r\n      nickName: \"\",\r\n      session: \"\",\r\n      sessionData: \"\",\r\n      search: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    this.session = JSON.parse(window.sessionStorage.getItem(\"user\"));\r\n    this.nickName = this.session.nickName;\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.el-dropdown-link {\r\n  cursor: pointer;\r\n  color: #409eff;\r\n}\r\n.el-icon-arrow-down {\r\n  font-size: 12px;\r\n}\r\n.el-carousel__item h3 {\r\n  color: #475669;\r\n  font-size: 18px;\r\n  opacity: 0.75;\r\n  line-height: 300px;\r\n  margin: 0;\r\n}\r\n.el-carousel__item:nth-child(2n) {\r\n  background-color: #99a9bf;\r\n}\r\n\r\n.el-carousel__item:nth-child(2n + 1) {\r\n  background-color: #d3dce6;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}