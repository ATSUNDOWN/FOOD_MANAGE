<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ysj.mapper.OrderMapper">
<!--    void updateOrderStatus(@Param("status") Integer status);-->
    <update id="updateOrderStatus">
        update ordering
        <set>
            status = #{status}
        </set>
        where id = #{id}
    </update>
    <select id="checkMyOrder" resultType="com.ysj.entity.Food">
        SELECT * from ordering
        where username= #{username}
    </select>
</mapper>

